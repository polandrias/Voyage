@model IEnumerable<Voyage.Models.User>

@{
    ViewBag.Title = "Opret bruger";
    Layout = "~/Areas/Admin/Views/Shared/_Layout.cshtml";
}

<h2>@ViewBag.Title</h2>
@*<p>
    @Html.ActionLink("Til hovedside", "Index", "Home")
</p>*@

<p>
    @Html.ActionLink("Opret bruger", "Create")
</p>
<table class="table table-striped">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Firstname)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Lastname)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Email)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Phone)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Password)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Firstname)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Lastname)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Email)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Phone)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Password)
            </td>
            <td>
                @Html.ActionLink("Opdatér", "Edit", new { id = item.ID }) |
                @Html.ActionLink("Detaljer", "Details", new { id = item.ID }) |
                @Html.ActionLink("Slet", "Delete", new { id = item.ID })
            </td>
        </tr>
    }
</table>
